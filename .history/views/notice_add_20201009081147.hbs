{{!< main}}


<head>

  <link type="application/atom+xml" rel="alternate" href="https://quilljs.com/feed.xml"
    title="Quill - Your powerful rich text editor">
  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.css">
  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/monokai-sublime.min.css">
  <link rel="stylesheet" href="//cdn.quilljs.com/1.3.6/quill.snow.css">




</head>
<form class="ui form" method="POST" action="/notice/add">
  <div class="field">
    <label>Title</label>
    <input type="text" name="first-name" placeholder="Title">
  </div>

  <button class="ui button" type="submit">Submit</button>
</form>
  <div id="snow-container" class="ql-container ql-snow">
    <div class="ql-editor" data-gramm="false" contenteditable="true" data-placeholder="Compose an epic...">
      <p>asdf</p>
    </div>
    <div class="ql-clipboard" contenteditable="true" tabindex="-1"></div>
    <div class="ql-tooltip ql-hidden"><a class="ql-preview" target="_blank" href="about:blank"></a><input type="text"
        data-formula="e=mc^2" data-link="https://quilljs.com" data-video="Embed URL"><a class="ql-action"></a><a
        class="ql-remove"></a></div>
  </div>
<script type="text/javascript">window["_gaUserPrefs"] = { ioo: function () { return true; } }</script>
<script src="//cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.js"></script>
