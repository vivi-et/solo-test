{{!< main}}

<h1>Article home</h1>


<br>
<br>
<table class="ui celled table">
  <thead>
    <tr>
      <th>ID</th>
      <th>User</th>
      <th>Title</th>
      {{!-- <th>Text</th> --}}
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    {{#each articles.rows}}
    <tr>
      <td>{{id}}</td>
      <td>{{user.username}}</td>
      <td><a href="/article/view/{{id}}">{{title}}</a></td>
      {{!-- <td>{{text}}</td> --}}
      <td>{{moments}}</td>
    </tr>
    {{/each}}
    
  </tbody>
</table>
<div class="ui pagination menu">
  <a class="active item">
    <-
  </a>
  <div class="disabled item">
